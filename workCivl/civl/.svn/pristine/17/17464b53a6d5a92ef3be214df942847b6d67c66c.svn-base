#include <stdlib.h>
#include <stddef.h>
#include <civlc.cvh>

$scope root = $here;

int main(){
  ptrdiff_t diff;
  double ** x;

  x = (double **)$malloc(root, sizeof(double *) * 3);
  for(int i=0; i<3; i++)
    x[i] = (double *)$malloc(root, sizeof(double) * 2);

  diff = x[1] - x[0];

  for(int i=0; i<3; i++)
    $free(x[i]);
  $free(x);

  return 0;
}


