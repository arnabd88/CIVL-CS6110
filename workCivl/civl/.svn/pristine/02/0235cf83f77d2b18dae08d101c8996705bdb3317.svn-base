/* Commandline execution:
 *		civl verify arrays.cvl
 * */
#include<civlc.cvh>

void main() {
  int a[1][1];
  int b[2] = {[1]=7};
  int c[2][2] = {[1]={[1]=1}};
  
  $assert((c[1][1]==1));
  $assert((b[0]==0));
  $assert((b[1]==7));
  a[0][0] = 2;
  b[1] = 5;
  $assert((a[0][0] == 2));
  $assert((b[1] == 5));
}
