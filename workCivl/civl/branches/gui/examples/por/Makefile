CIVL=civl
VERIFY=$(CIVL) verify

all: atomic0 atomic1 pointerShare pointerShare1 pointerShare2 trade3 trade4 guard1 guard2 wait

atomic0 : atomic0.cvl
	$(VERIFY) atomic0.cvl

atomic1 : atomic1.cvl
	$(VERIFY) atomic1.cvl

pointerShare : pointerShare.cvl
	$(VERIFY) pointerShare.cvl

pointerShare1 : pointerShare1.cvl
	$(VERIFY) pointerShare1.cvl

pointerShare2 : pointerShare2.cvl
	$(VERIFY) pointerShare2.cvl

trade3 : trade3.cvl
	$(VERIFY) trade3.cvl

trade4 : trade4.cvl
	$(VERIFY) trade4.cvl

guard1 : guard1.cvl
	$(VERIFY) guard1.cvl

guard2 : guard2.cvl
	$(VERIFY) guard2.cvl

wait : wait.cvl
	$(VERIFY) wait.cvl

