#include<civlc.cvh>
#include<stdio.h>
int fooLength = $textFileLength("foo");

$assume fooLength == 5;
//$assume $textFileLength("foo") == 5;

void main(){
  FILE *file = fopen("foo", "r");
  int a[5];
  int i = 0;
  
  while($true){
    int count = fscanf(file, "%d", &a[i]);
    
    if(count == EOF)
      break;
    i++;
  } 
  $free(file);
}
