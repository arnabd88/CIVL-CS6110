# This properties file will work fine for most users.  
# If you need to change anything, create your own file 
# called build.properties in this directory.  
# It will be read automatically in place of this one.
root=/opt/vsl
workspace=/Users/${user.name}/Documents/workspace
antlr.jar=${root}/lib/antlr-complete.jar
antlr.runtime.jar=${root}/lib/antlr-runtime.jar
hamcrest.jar=${root}/lib/hamcrest-core.jar
jacoco.jar=${root}/src/jacoco/lib/jacocoant.jar
junit.jar=${root}/lib/junit.jar
sarl.jar=${workspace}/SARL/sarl.jar
