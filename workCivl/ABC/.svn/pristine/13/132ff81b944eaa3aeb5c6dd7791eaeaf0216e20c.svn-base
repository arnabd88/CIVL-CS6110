package edu.udel.cis.vsl.abc.ast.node.IF.statement;

import edu.udel.cis.vsl.abc.ast.node.IF.SequenceNode;

/**
 * A compound statement is a sequence of statements and declarations within
 * curly braces: "{ ... }". It is also known as a "block". The items that can
 * occur with a block, i.e., declarations and statements, are known as
 * "block items".
 * 
 * @author siegel
 * 
 */
public interface CompoundStatementNode extends StatementNode,
		SequenceNode<BlockItemNode> {

	@Override
	CompoundStatementNode copy();

}
